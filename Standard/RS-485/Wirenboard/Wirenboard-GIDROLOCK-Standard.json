                "title": "Response delay to a modbus request (ms)",
                "group": "g_general",
                "address": 113,
                "order": 2,
                "reg_type": "holding",
                "min": 0,
                "max": 254,
                "default": 0
            },
            "Modbus_Address": {
                "title": "Device Modbus Address",
                "group": "g_general",
                "description": "Modbus_Address_description",
                "address": 128,
                "order": 3,
                "reg_type": "holding",
                "min": 0,
                "max": 254
